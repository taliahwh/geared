{
  "expo": {
    "name": "geared",
    "slug": "geared",
    "owner": "taliahwh",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/geared_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-screen-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#222222"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with other users."
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.taliahwharton.geared"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.taliahwharton.geared"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
